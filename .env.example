# API Configuration
API_BASE_URL=https://api.fitanalyzer.com
API_VERSION=v1
API_TIMEOUT=30000

# Authentication
JWT_SECRET_KEY=your_jwt_secret_key_here
REFRESH_TOKEN_EXPIRY=7d
ACCESS_TOKEN_EXPIRY=1h

# Video Processing
MAX_VIDEO_SIZE_MB=100
MAX_VIDEO_DURATION_SECONDS=300
VIDEO_QUALITY=720p
FRAME_RATE=30

# MediaPipe Configuration
MEDIAPIPE_MODEL_COMPLEXITY=1
MEDIAPIPE_MIN_DETECTION_CONFIDENCE=0.5
MEDIAPIPE_MIN_TRACKING_CONFIDENCE=0.5

# Storage
CLOUD_STORAGE_BUCKET=fitanalyzer-videos
LOCAL_STORAGE_PATH=/data/videos

# Analytics
ANALYTICS_ENABLED=true
ANALYTICS_API_KEY=your_analytics_key

# Push Notifications
FCM_SERVER_KEY=your_fcm_server_key
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id

# Payment
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret

# Social Login
GOOGLE_CLIENT_ID=your_google_client_id
FACEBOOK_APP_ID=your_facebook_app_id
APPLE_CLIENT_ID=your_apple_client_id

# Development
DEBUG_MODE=true
LOG_LEVEL=debug
FLIPPER_ENABLED=true

# iOS Specific
APPLE_TEAM_ID=your_apple_team_id
BUNDLE_IDENTIFIER=com.fitanalyzer.pro

# Android Specific
ANDROID_PACKAGE_NAME=com.fitanalyzer.pro
ANDROID_KEYSTORE_PASSWORD=your_keystore_password

# HealthKit (iOS)
HEALTHKIT_ENABLED=true
HEALTHKIT_PERMISSIONS=steps,distance,calories

# Camera
CAMERA_QUALITY=720p
CAMERA_FPS=30
CAMERA_STABILIZATION=true

# Machine Learning
ML_MODEL_VERSION=1.0.0
ML_INFERENCE_TIMEOUT=5000
ML_BATCH_SIZE=1

# Cache
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_MAX_SIZE=100MB

# Monitoring
SENTRY_DSN=your_sentry_dsn
BUGSNAG_API_KEY=your_bugsnag_key

# Feature Flags
FEATURE_PREMIUM_ANALYSIS=true
FEATURE_SOCIAL_SHARING=true
FEATURE_OFFLINE_MODE=true
FEATURE_BIOMETRIC_AUTH=true